import java.lang.*;
public class BasicCalculator implements Calculation {
	
	private double value1;
	private double value2;
	public double value3;
	
	public void setValue1(double value1)
	{
		this.value1=value1;
	}
	
	public double getValue1()
	{
		return value1;
	}
	
	public void setValue2(double value2)
	{
		this.value2=value2;
	}
	
	public double getValue2()
	{
		return value2;
	}
	
	public void add()
	{
		value3=value1+value2;
		System.out.println("Addition : "+value3);
	}
	
	public void subtract()
	{
		value3=value1-value2;
		System.out.println("Subtract : "+value3);
	}
	
	public void multiply()
	{
		value3=value1*value2;
		System.out.println("Multiplycation : "+value3);
	}
	
	public void divide()
	{
		value3=value1/value2;
		System.out.println("Divition : "+value3);
	}
	
	BasicCalculator( )
	{
		
	}
	
	BasicCalculator(double value1, double value2)
	{
		value1=value1;
		value2=value2;
	}
}
